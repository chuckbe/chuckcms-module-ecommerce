{
    "name": "chuckbe/chuckcms-module-ecommerce",
    "description": "E-commerce module for use in ChuckCMS",
    "keywords": ["chuckbe", "chuckcms", "cms", "laravel", "php"],
    "homepage": "https://github.com/chuckbe/chuckcms-module-ecommerce",
    "require": {
        "php": ">=7.2.0",
        "laravel/framework": "~5.8||^6.18.35||^7.30.4",
        "barryvdh/laravel-dompdf": "^0.8.1",
        "chuckbe/chuckcms": "~0.1",
        "mollie/laravel-mollie": "^2.0",
        "maatwebsite/excel": "^3.1"
    },
    "authors": [
        {
            "name": "Karel Brijs",
            "email": "karel@chuck.be"
        }
    ],
    "autoload": {
        "psr-4": {
            "Chuckbe\\ChuckcmsModuleEcommerce\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chuckbe\\ChuckcmsModuleEcommerce\\ChuckcmsModuleEcommerceServiceProvider",
                "Maatwebsite\\Excel\\ExcelServiceProvider"
            ],
            "aliases": {
                "ChuckCart": "Chuckbe\\ChuckcmsModuleEcommerce\\Facades\\Cart", 
                "ChuckCollection": "Chuckbe\\ChuckcmsModuleEcommerce\\Facades\\Collection", 
                "ChuckCustomer": "Chuckbe\\ChuckcmsModuleEcommerce\\Facades\\Customer", 
                "ChuckEcommerce": "Chuckbe\\ChuckcmsModuleEcommerce\\Facades\\Ecommerce", 
                "ChuckProduct": "Chuckbe\\ChuckcmsModuleEcommerce\\Facades\\Product",
                "Excel": "Maatwebsite\\Excel\\Facades\\Excel"
            }
        }
    },
    "minimum-stability": "dev"
}
